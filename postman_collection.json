              "mode": "raw",
              "raw": "{\n  \"name\": \"Summer Stream Giveaway 2024\",\n  \"description\": \"A summer stream giveaway for all subscribers\",\n  \"status\": \"OPEN\",\n  \"platforms\": [\"TWITCH\", \"KICK\", \"YOUTUBE\"],\n  \"keyword\": \"!enter\",\n  \"allowedRoles\": [\"TWITCH_TIER_1\", \"TWITCH_TIER_2\", \"TWITCH_TIER_3\", \"TWITCH_NON_SUB\", \"KICK_SUB\", \"KICK_NON_SUB\", \"YOUTUBE_SUB\", \"YOUTUBE_NON_SUB\"],\n  \"ticketRuleOverrides\": [\n    {\n      \"role\": \"TWITCH_TIER_3\",\n      \"ticketsPerUnit\": 25\n    }\n  ],\n  \"donationConfigs\": [\n    {\n      \"platform\": \"TWITCH\",\n      \"unitType\": \"BITS\",\n      \"donationWindow\": \"DAILY\"\n    },\n    {\n      \"platform\": \"TWITCH\",\n      \"unitType\": \"GIFT_SUB\",\n      \"donationWindow\": \"DAILY\"\n    },\n    {\n      \"platform\": \"KICK\",\n      \"unitType\": \"KICK_COINS\",\n      \"donationWindow\": \"DAILY\"\n    },\n    {\n      \"platform\": \"YOUTUBE\",\n      \"unitType\": \"SUPERCHAT\",\n      \"donationWindow\": \"DAILY\"\n    }\n  ],\n  \"donationRuleOverrides\": [\n    {\n      \"platform\": \"TWITCH\",\n      \"unitType\": \"BITS\",\n      \"unitSize\": 100,\n      \"ticketsPerUnitSize\": 1\n    }\n  ]\n}"
            },
            "url": {
              "raw": "{{base_url}}/stream-giveaways",
              "host": ["{{base_url}}"],
              "path": ["stream-giveaways"]
